{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Stock": {
      "type": "integer",
      "required": true,
      "default": 0,
      "unique": false,
      "min": 0
    },
    "Cost": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "Price": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "Estado": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "products_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "brands": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand.brand"
    },
    "stores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::store.store"
    }
  }
}
